programa
{
	
	funcao inicio()
	{
	inteiro numero1
	escreva("Qual sua idade? ")
	leia (numero1)
	se (numero1 <= 18)
	escreva("Não pode dirigir")
	senao
	escreva("Pode dirigir")
	}
}
